{
  "name": "todoist-nodejs-sdk",
  "version": "0.0.11",
  "private": false,
  "description": "Unofficial Todoist SDK for Node.js",
  "keywords": [
    "Todoist",
    "SDK",
    "Node.js"
  ],
  "homepage": "https://github.com/mds2000/todoist-nodejs-sdk#readme",
  "bugs": {
    "url": "https://github.com/mds2000/todoist-nodejs-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mds2000/todoist-nodejs-sdk.git"
  },
  "license": "MIT",
  "author": "Mat√≠as da Silva",
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npx tsx example.ts",
    "build": "tsdown index.ts --format cjs,esm --dts",
    "release": "npm run build && npx changeset publish",
    "lint": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "add-version": "npx changeset"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsdown": "^0.15.7",
    "typescript": "^5.9.3"
  }
}